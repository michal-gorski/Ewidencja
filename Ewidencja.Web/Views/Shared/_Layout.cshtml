<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <!-- <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic,900|Roboto+Condensed:300italic,400italic,700italic,400,300,700&subset=latin,latin-ext' rel='stylesheet' type='text/css'> -->
    
    <title>@ViewBag.Title</title>
    @Styles.Render("~/bundles/bootstrapCSS")    
    @Styles.Render("~/bundles/fontAwesome")  
    @Styles.Render("~/bundles/styles")  
    
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrapJS")
    @Scripts.Render("~/bundles/angular")
    
    @Scripts.Render("~/bundles/ewidencjaScriptControllers")
    @Scripts.Render("~/bundles/ewidencjaScriptServices")
    @Scripts.Render("~/bundles/ewidencjaScript")
    
    @RenderSection("scripts", required: false)
    
</head>
<body>
    <div id="container">
        <div id="header">
            
            <img id="logo" src="~/Images/logo.png" />
            <span id="title">Ewidencja Pacjentów</span>
            @Html.ActionLink("WYLOGUJ","Logoff", "Account", new {@class="font-bold",id="logout" })
        </div>
        <div id="app-content" ng-app="ewidencja">
            
            @RenderBody()    

        </div>
        
    </div>

</body>
</html>

